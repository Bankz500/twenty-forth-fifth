{
  "hosting": [
    {
      "target": "twenty-third-forth",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/testimonials",
          "destination": "/testimonials.html"
        },
        {
          "source": "/testimonials/**",
          "destination": "/testimonials.html"
        },
        {
          "source": "/terms",
          "destination": "/terms.html"
        },
        {
          "source": "/terms.html",
          "destination": "/terms.html"
        },
        {
          "source": "/privacy",
          "destination": "/privacy.html"
        },
        {
          "source": "/privacy.html",
          "destination": "/privacy.html"
        },
        {
          "source": "/policy",
          "destination": "/policy.html"
        },
        {
          "source": "/policy.html",
          "destination": "/policy.html"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "headers": [
        {
          "source": "/sitemap.xml",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/xml"
            },
            {
              "key": "Cache-Control",
              "value": "public, max-age=3600"
            }
          ]
        },
        {
          "source": "/robots.txt",
          "headers": [
            {
              "key": "Content-Type",
              "value": "text/plain"
            },
            {
              "key": "Cache-Control",
              "value": "public, max-age=3600"
            }
          ]
        },
        {
          "source": "/testimonials.html",
          "headers": [
            {
              "key": "X-Robots-Tag",
              "value": "index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
            }
          ]
        },
        {
          "source": "/testimonials",
          "headers": [
            {
              "key": "X-Robots-Tag",
              "value": "index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
            }
          ]
        }
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules"
  }
}
